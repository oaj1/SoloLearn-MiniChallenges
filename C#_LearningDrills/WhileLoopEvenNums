using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            int num = Convert.ToInt32(Console.ReadLine());
            // int res = 1;
            //your code goes here
            
            int  i = 1;
            while (i <= num) 
                {
                    if (i % 2 == 0)
                    {
                        Console.WriteLine(i);
                        
                    }    
                    i++;
                }
            
        }
    }
}
